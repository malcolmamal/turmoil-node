{
  "name": "turmoil-backend",
  "version": "1.0.0",
  "type": "module",
  "description": "turmoil-backend",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "pretest": "npm run db:reset",
    "lint": "node ./node_modules/eslint/bin/eslint.js src --ext=js",
    "lint:fix": "npm run lint -- --fix",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "create-migration": "node src/utils/create-initial-migration.js",
    "check-updates": "npx npm-check-updates -u"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.2",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "http-status-codes": "^2.2.0",
    "joi": "^17.6.0",
    "js-beautify": "^1.14.0",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^3.2.3",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.17.0",
    "turmoil": "file:.."
  },
  "devDependencies": {
    "@jest/globals": "^27.5.1",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.3",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "sequelize-automated-migrations": "^1.0.0",
    "supertest": "^6.2.2"
  }
}
